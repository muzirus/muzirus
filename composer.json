{
    "name": "petrofcikmatus/dictionary",
    "description": "",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.1.3",
        "ext-apcu": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-mbstring": "*",
        "cocur/slugify": "^3.1",
        "erusev/parsedown": "^1.7",
        "ezyang/htmlpurifier": "^4.10",
        "sensio/framework-extra-bundle": "^5.1",
        "sensiolabs/security-checker": "^4.1",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "^4.0",
        "symfony/console": "^4.0",
        "symfony/debug-pack": "*",
        "symfony/expression-language": "^4.0",
        "symfony/flex": "^1.0",
        "symfony/form": "^4.0",
        "symfony/framework-bundle": "^4.0",
        "symfony/lts": "^4@dev",
        "symfony/maker-bundle": "^1.0",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "^1.0",
        "symfony/process": "^4.0",
        "symfony/security-bundle": "^4.0",
        "symfony/serializer-pack": "*",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/translation": "^4.0",
        "symfony/validator": "^4.0",
        "symfony/web-link": "^4.0",
        "symfony/webpack-encore-pack": "*",
        "symfony/yaml": "^4.0",
        "white-october/pagerfanta-bundle": "^1.1"
    },
    "require-dev": {
        "heroku/heroku-buildpack-php": "*",
        "lmc/coding-standard": "^1.0",
        "symfony/browser-kit": "^4.0",
        "symfony/css-selector": "^4.0",
        "symfony/dotenv": "^4.0",
        "symfony/phpunit-bridge": "^4.0",
        "symfony/profiler-pack": "*",
        "symfony/web-server-bundle": "^4.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "security-checker security:check": "script"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "prod": [
            "@composer install --no-dev --optimize-autoloader",
            "@composer clear-cache"
        ],
        "dev": [
            "@composer install"
        ],
        "compile": [
            "@php bin/console doctrine:migrations:migrate --no-interaction --allow-no-migration"
        ],
        "cs": [
            "vendor/bin/ecs check src/ tests/ --ansi"
        ],
        "cs-fix": [
            "vendor/bin/ecs check src/ tests/ --ansi --fix"
        ],
        "phpunit": "./vendor/bin/simple-phpunit",
        "analyze": [
            "@composer cs",
            "@composer phpunit"
        ],
        "translation-cs-yml": "@php bin/console translation:update --force --clean --domain=messages --output-format=yml cs",
        "translation-cs-xlf": "@php bin/console translation:update --force --clean --domain=messages --output-format=xlf cs",
        "translation-en-yml": "@php bin/console translation:update --force --clean --domain=messages --output-format=yml en",
        "translation-en-xlf": "@php bin/console translation:update --force --clean --domain=messages --output-format=xlf en",
        "translation-ru-yml": "@php bin/console translation:update --force --clean --domain=messages --output-format=yml ru",
        "translation-ru-xlf": "@php bin/console translation:update --force --clean --domain=messages --output-format=xlf ru"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "id": "01C5H95BKEGJF8VQ5H9F02MXM5",
            "allow-contrib": false
        }
    }
}
